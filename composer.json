{
  "name": "msbios/theme",
  "description": "MSBios Theme",
  "license": "GPL",
  "keywords": [
    "php"
  ],
  "homepage": "https://github.com/msbios/msbios-theme",
  "minimum-stability": "stable",
  "authors": [
    {
      "name": "Judzhin Miles",
      "email": "info@msbios.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "MSBios\\Theme\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MSBiosTest\\Theme\\": "tests/"
    }
  },
  "require": {
    "zendframework/zend-loader": "^2.5",
    "zendframework/zend-servicemanager": "^3.3",
    "zendframework/zend-modulemanager": "^2.8",
    "msbios/msbios": "~1.0",
    "msbios/widget": "~1.0",
    "msbios/assetic": "~1.0",
    "msbios/stdlib": "~1.0",
    "msbios/i18n": "~1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7",
    "zendframework/zend-test": "^3.1",
    "squizlabs/php_codesniffer": "^3.0",
    "msbios/debug": "^1.0"
  },
  "scripts": {
    "cs-check": "phpcs",
    "cs-fix": "phpcbf",
    "test": [
      "@cs-check",
      "phpunit"
    ],
    "serve": "php -S 0.0.0.0:3107 -t bin bin/msbios-theme-module.php",
    "commit": "git add . && git commit -a -m \"Lazy Composer Commiter\" && git push"
  }
}
